ENTRY(_start)

SECTIONS
{
    . = 0x0;
    .text :
    {
        __text_start = .;
        *(.text.boot) *(.text);
    }
    .rodata :
    {
        *(.rodata);
    }
    .rela :
    {
        __rela_start = .;
        *(.rela.*);
        __rela_end = .;
    }
    .data :
    {
        *(.data);
    }
    .bss :
    {
        __bss_start = .;
        *(.bss);
        __bss_end = .;
    }
    _image_end = .;
}
